{
    "name": "graviton/graviton-service-bundle-audit-tracking",
    "license": "GPL",
    "type": "project",
    "description": "Audit tracking bundle to be used in graviton/graviton",
    "authors": [
        {
            "name": "List of contributors",
            "homepage": "https://github.com/libgraviton/graviton/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "autoload": {
        "psr-0": { "Graviton": "src/"}
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "squizlabs/php_codesniffer": "~2.6",
        "libgraviton/codesniffer": "~1.3"
    },
    "scripts": {
        "check": [
            "vendor/bin/graviton-validate-directory --path .",
            "./vendor/bin/phpcs --config-set installed_paths ../../libgraviton/codesniffer/CodeSniffer/Standards",
            "./vendor/bin/phpcs --standard=PSR1 src/",
            "./vendor/bin/phpcs --standard=PSR2 src/",
            "./vendor/bin/phpcs --standard=ENTB src/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
